package concepts;

//1. Atleast one empty arg constructor
//2. Data is encapsulated

public class JBDemo {
	//Attributes are encapsulated 
	private int x;
	private String y;
	public JBDemo() {
		
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public String getY() {
		return y;
	}
	public void setY(String y) {
		this.y = y;
	}
	
	

}
